// Create a cycle of size 3, four times

struct %node {
  %node ptr;
};

%node n1;
%node n2;
%node n3;

n1 := new %node;
n2 := new %node;
n3 := new %node;
n1.ptr := n2;
n2.ptr := n3;
n3.ptr := n1;

n1 := new %node;
n2 := new %node;
n3 := new %node;
n1.ptr := n2;
n2.ptr := n3;
n3.ptr := n1;

n1 := new %node;
n2 := new %node;
n3 := new %node;
n1.ptr := n2;
n2.ptr := n3;
n3.ptr := n1;

n1 := new %node;
n2 := new %node;
n3 := new %node;
n1.ptr := n2;
n2.ptr := n3;
n3.ptr := n1;

output 3;
